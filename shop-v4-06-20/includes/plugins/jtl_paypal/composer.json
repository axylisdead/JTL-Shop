{
    "name": "jtlshop/paypal",
    "type": "library",
    "version": "1.0.9",
    "description": "JTL-Shop Plugin",
    "require": {
        "php": ">=5.4.0",
        "paypal/rest-api-sdk-php": "^1.13",
        "paypal/merchant-sdk-php": "v3.9.1"
    },
    "autoload": {
        "psr-4": {
            "PayPal\\": "lib/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/PayPal"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5 || ^5.0.5",
        "phpunit/phpunit-mock-objects": "2.3.0 || ^3.0",
        "symfony/finder": "^3.2"
    },
    "scripts": {
        "test": "phpunit"
    }
}
